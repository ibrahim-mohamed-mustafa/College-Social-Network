@model College_Social_Network_Project.Models.Admin

@{
    ViewBag.Title = "Create";
}

<link href="~/Content/StyleSheet2.css" rel="stylesheet" />
@Html.Partial("_Dashboard")

<h1 id="Duser">Update My Profile</h1>


@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    <hr />

    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.HiddenFor(model => model.Aid)

    <div class="form-group">
        @Html.LabelFor(model => model.Aemail, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.Aemail, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
            @Html.ValidationMessageFor(model => model.Aemail, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Aaddress, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.Aaddress, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Aaddress, "", new { @class = "text-danger" })
        </div>
    </div>

    <div class="form-group">
        @Html.LabelFor(model => model.Agender, htmlAttributes: new { @class = "control-label col-md-2" })
        <div class="col-md-10">
            @Html.EditorFor(model => model.Agender, new { htmlAttributes = new { @class = "form-control" } })
            @Html.ValidationMessageFor(model => model.Agender, "", new { @class = "text-danger" })
        </div>
    </div>


    <div class="form" id="updateform">
        <section class="loginForm">

            <div class="form-group">
                <div class="col-md-offset-4 col-md-3">
                    <input type="submit" value="Save" class="button-55" id="save" />
                    <br /><br />
                </div>

                <div class="col-md-offset-4 col-md-5">
                    @Html.ActionLink("Cancel", "Index", null, new { @class = "button-55" })
                    <br /><br />
                </div>
            </div>

        </section>
    </div>

</div>
}



@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
