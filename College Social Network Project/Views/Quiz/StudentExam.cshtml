
@{
    ViewBag.Title = "StudentExam";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StudentExam</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/StyleSheet2.css" rel="stylesheet" />


</head>


<body>
   
    @if (@ViewBag.error != null)
    {
        <div class="alert alert-danger">
            <strong id="examroom">Error!</strong> @ViewBag.error
        </div>
    }
    <img id="quizroom" src="/Images/quizroom3.jpg" alt="">

    <div id="quizroom">
        @using (Html.BeginForm())
        {
            <div class="container">
                <br />
                <br />
                <h1 id="Duserroom">Enter Room Number</h1>
                <input type="text" id="room" name="room" placeholder="Room Number" class="form-control" />
                <br />
                <br />
                <input type="submit" value="Submit" class="button-55" id="sumbitroom" />
            </div>
        }
    </div>



</body>
</html>